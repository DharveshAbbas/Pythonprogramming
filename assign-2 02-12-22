#way 1
def vote(n):
    if n>18:
        print('yes')
    else:
        print('No')
a=int(input("Enter age: "))
vote(a)

#way 2
def vote(n):
    if n>18:
        return True
    else:
        return False
a=int(input("Enter your age: "))
if(vote(a)):
    print("yes")
else:
    print("no")
#way 3
def vote(n):
    if n>18:
        return "yes"
    else:
        return "no"
a=int(input("Enter your age: "))
print(vote(a))
