sno=int(input("Enter student number"))
name=input("Enter student name")
gender=input("Enter student gender ")
group=input("Enter student group ")
adress=input("Enter student adress")
phoneno=int(input("Enter student phone number"))
print("student number is= ",sno)
print("student name is= ",name)
print("student gender is= ",gender)
print("student group is= ",group)
print("student adress is= ",adress)
print("student phone number is= ",phoneno)
